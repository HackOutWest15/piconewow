{
  "rules": {
    "new-cap": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "jsx-quotes": [2, "prefer-single"],
    "no-use-before-define": 0,
    "no-duplicate-imports": 0,
    "no-restricted-syntax": 0,
    "arrow-body-style": 0,
    "padded-blocks": ["error", { "classes": "always" }]
  }
}
