<!DOCTYPE html>
<html class="stage">
    <head>
        <title>PicOne - Way out West</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
        <script src="lib/hammer/hammer.min.js"></script>
        <script src="lib/jQuery.succinct.min.js"></script>
        <script src="lib/underscore-min.js"></script>
        <script src="//connect.facebook.net/en_US/sdk.js#xfbml=1&appId=1638718853011507&version=v2.0"></script>
        <script src="javascripts/login.js"></script>
        <script src="javascripts/spotify.js"></script>
        <script src="/javascripts/tindercards.js"></script>
        <script src="/javascripts/swipe.js"></script>
        <script src="/javascripts/card-flip.js"></script>

        <link href='http://fonts.googleapis.com/css?family=Pacifico' rel='stylesheet' type='text/css'>

        <link rel="stylesheet" type="text/css" href="stylesheets/style.css">
    </head>
    <body>
        <script>
            var unseenStr = '<%-unseen%>';
            var unseen = JSON.parse(unseenStr);
        </script>
        <section id="Screen" class="Screen">
          <div class="Wizard-box">
                <div class="u-center-vertical-wrap">
                    <div class="u-center-vertical">
                        <div class="Wizard-content">
                            <h2>How to pic</h2>
                            <div class="Wizard-row">
                                <div class="Wizard-text">Swipe right if you want to add the artist to your schedule</div>
                                <div class="Wizard-img">
                                    <img src="img/swipe-right.png">
                                </div>
                            </div>
                            <div class="Wizard-row">
                                <div class="Wizard-img">
                                    <img src="img/swipe-left.png">
                                </div>
                                <div class="Wizard-text">Swipe left if you don't want to watch the artist</div>
                            </div>
                            <div class="Wizard-text-full">Touch the cover to see more information about the artist and who of your friends are attending</div>
                            <div class="Button-simple u-center-horizontal">Close</div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="feedback-yes" class="feedback-box">
                <img width="100px" src="/images/feedback_yes.svg" />
            </div>
            <div id="feedback-no" class="feedback-box">
                <img width="100px" src="/images/feedback_no.svg" />
            </div>
        </section>
    </body>
</html>